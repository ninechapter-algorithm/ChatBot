{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": false,
  "printWidth": 100,
  "trailingComma": "all",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "useTabs": false,
  "quoteProps": "as-needed",
  "rangeStart": 0,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css"
}
